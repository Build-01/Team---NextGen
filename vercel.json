{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "HealthBud_Frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/",
      "dest": "/HealthBud_Frontend/Index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/HealthBud_Frontend/$1"
    }
  ]
}